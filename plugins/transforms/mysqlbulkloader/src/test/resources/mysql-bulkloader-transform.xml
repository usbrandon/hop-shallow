<transform>
    <name>MySQL bulk loader</name>
    <type>MySqlBulkLoader</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
        <method>none</method>
        <schema_name/>
    </partitioning>
    <bulk_size/>
    <connection>mysql</connection>
    <delimiter>	</delimiter>
    <enclosure>"</enclosure>
    <encoding/>
    <escape_char>\</escape_char>
    <fields>
        <field>
            <field_format_ok>OK</field_format_ok>
            <field_name>column1</field_name>
            <stream_name>column1</stream_name>
        </field>
        <field>
            <field_format_ok>OK</field_format_ok>
            <field_name>column2</field_name>
            <stream_name>column2</stream_name>
        </field>
    </fields>
    <fifo_file_name>/tmp/fifo</fifo_file_name>
    <ignore>N</ignore>
    <local>Y</local>
    <replace>N</replace>
    <schema/>
    <table>testTable</table>
    <attributes/>
    <GUI>
        <xloc>464</xloc>
        <yloc>160</yloc>
    </GUI>
</transform>
