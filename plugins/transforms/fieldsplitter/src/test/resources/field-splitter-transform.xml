<transform>
    <name>Split value</name>
    <type>FieldSplitter</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
        <method>none</method>
        <schema_name/>
    </partitioning>
    <splitfield>value</splitfield>
    <delimiter>,</delimiter>
    <enclosure/>
    <escape_string/>
    <fields>
        <field>
            <name>a</name>
            <id>a=</id>
            <idrem>Y</idrem>
            <type>Integer</type>
            <format>#</format>
            <group/>
            <decimal/>
            <currency/>
            <length>-1</length>
            <precision>-1</precision>
            <nullif/>
            <ifnull/>
            <trimtype>both</trimtype>
        </field>
        <field>
            <name>b</name>
            <id>b=</id>
            <idrem>Y</idrem>
            <type>Integer</type>
            <format>#</format>
            <group/>
            <decimal/>
            <currency/>
            <length>-1</length>
            <precision>-1</precision>
            <nullif/>
            <ifnull/>
            <trimtype>both</trimtype>
        </field>
        <field>
            <name>c</name>
            <id>c=</id>
            <idrem>Y</idrem>
            <type>Integer</type>
            <format>#</format>
            <group/>
            <decimal/>
            <currency/>
            <length>-1</length>
            <precision>-1</precision>
            <nullif/>
            <ifnull/>
            <trimtype>both</trimtype>
        </field>
        <field>
            <name>d</name>
            <id>d=</id>
            <idrem>Y</idrem>
            <type>Integer</type>
            <format>#</format>
            <group/>
            <decimal/>
            <currency/>
            <length>-1</length>
            <precision>-1</precision>
            <nullif/>
            <ifnull/>
            <trimtype>both</trimtype>
        </field>
    </fields>
    <attributes></attributes>
    <GUI>
        <xloc>256</xloc>
        <yloc>80</yloc>
    </GUI>
</transform>
