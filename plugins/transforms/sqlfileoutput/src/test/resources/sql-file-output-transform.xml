<transform>
    <name>SQL file output</name>
    <type>SQLFileOutput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
        <method>none</method>
        <schema_name/>
    </partitioning>
    <connection>${DATABASE_NAME}</connection>
    <schema>public</schema>
    <table>testtable</table>
    <truncate>N</truncate>
    <create>Y</create>
    <encoding>UTF-8</encoding>
    <dateformat/>
    <addtoresult>N</addtoresult>
    <startnewline>Y</startnewline>
    <file>
        <name>${PROJECT_HOME}/output/filename</name>
        <extention>sql</extention>
        <append>N</append>
        <split>N</split>
        <haspartno>N</haspartno>
        <add_date>N</add_date>
        <add_time>N</add_time>
        <splitevery>0</splitevery>
        <create_parent_folder>Y</create_parent_folder>
        <DoNotOpenNewFileInit>Y</DoNotOpenNewFileInit>
    </file>
    <attributes/>
    <GUI>
        <xloc>432</xloc>
        <yloc>112</yloc>
    </GUI>
</transform>
